<UserControl x:Class="ControlLibrary.Controls.FrequencyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ControlLibrary.Controls"
             mc:Ignorable="d" 
             Height="50" Width="331">
    <DockPanel VerticalAlignment="Center">
        <TextBlock Text="Введите частоту:" FontSize="16" Margin="5 0"/>
        <TextBox Name="FrequencyTextBox" FontSize="16" Margin="5 0" Text="" TextAlignment="Right" KeyUp="FrequencyConfirmed"/>        
    </DockPanel>
</UserControl>
